{
  "name": "tricore-lsp",
  "description": "Tricore language server",
  "author": "Xinquan XU",
  "license": "MIT",
  "version": "1.0.0",
  "publisher": "vscode",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:tricore",
    "workspaceContains:**/*.{s,S,asm,ASM}"
  ],
  "contributes": {
    "languages": [
      {
        "id": "Tricore Assembly",
        "aliases": [
          "Tricore Assembly",
          "Tricore Assembly"
        ],
        "extensions": [
          ".s",
          ".S",
          ".asm"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "Tricore Assembly",
        "scopeName": "source.tricore",
        "path": "./syntaxes/Tricore Assembly.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "typescript": "^4.8.3",
    "vscode-languageclient": "^7.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/vscode": "^1.63.0",
    "jest": "^29.1.2",
    "ts-jest": "^29.0.3",
    "vscode-oniguruma": "^1.6.2"
  }
}
